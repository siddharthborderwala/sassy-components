{
  "name": "sassy-components",
  "sideEffects": "false",
  "version": "0.1.0",
  "description": "A react ui component library based on shoelace",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf dist",
    "compile": "yarn run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c",
    "format": "prettier --write ./src/"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "12.12.2",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "jest": "^26.4.2",
    "jest-dom": "^4.0.0",
    "np": "^6.5.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "2.0.0-beta.19"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "tests/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
